generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User{
  id Int @unique @default(autoincrement())
  name String
  email String @unique
  password String
  profileImage String?
  createdAt DateTime @default(now())
  updateAt DateTime @updatedAt
}